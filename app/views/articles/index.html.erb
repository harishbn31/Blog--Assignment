<h2> Article</h2>


<%if @articles.empty? %>
<p> No artcles found</p>
<%else%>
<% @articles.each do | article| %>
<h2> <%= link_to article.title, article_path(article) %></h2>
<p> <%= article.body %></p><br/>
<%= article.published_date %>	
<%end%>

<%end%>
<%= link_to "New Article", new_article_path(@article) %>

